<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
       "http://struts.apache.org/dtds/struts-config_1_3.dtd">
 
 <struts-config>
 	<form-beans>
 		<form-bean name="student" type="com.models.Student"></form-bean>
 	</form-beans>
 	
 	<action-mappings>
 		<action path="/studentAction" name="student" type="com.controllers.StudentAction">
 			<forward name="display" path="/display.jsp"></forward>
 		</action>
 	</action-mappings>
 </struts-config>